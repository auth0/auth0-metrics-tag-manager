{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "vars-on-top": 0,
    "no-var": 0,
    "prefer-template": 0,
    "object-shorthand": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "consistent-return": 0,
    "no-unused-expressions": 0,
    "prefer-arrow-callback": 0,
    "prefer-rest-params": 0,
    "max-len": ["error", 255]
  },
  "eslintIgnore": ["lib"]
}
